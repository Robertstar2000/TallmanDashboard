    export interface SpreadsheetRow {
  id: string;
  name: string;
  serverName: 'P21' | 'POR';
  value: string | number;
  chartGroup: string;
  calculation: string;
  sqlExpression: string;      // For SQLite testing
  productionSqlExpression: string;  // For P21/POR SQL Server
  tableName: string;
  chartName?: string;
  variableName?: string;
  lastUpdated?: string;
  timeframe?: string;
}
  ];
